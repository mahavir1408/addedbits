@font-face {
  font-family: 'Muli';
  font-style: normal;
  font-weight: 400;  
}

.post {
color: #1E1E1E;
font-family: 'Muli',Helvetica,Arial,Georgia,Times New Roman;
font-size: 16px;
line-height: 24px;
text-align: justify;
text-shadow: 0 1px 0 #FFFFFF;
}

.top-bar {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: rgba(0, 0, 0, 0);
    background-image: url(https://lh3.googleusercontent.com/--y49xiXoG7g/VUxU3bhMF5I/AAAAAAAAAE4/rxK5kCGxE08/w426-h237/bar.png);
    background-origin: padding-box;
    background-position: center top;
    background-repeat: repeat;
    background-size: auto auto;
    height: 3px;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 9;
}

.plain-bar {
background: url(https://lh3.googleusercontent.com/--y49xiXoG7g/VUxU3bhMF5I/AAAAAAAAAE4/rxK5kCGxE08/w426-h237/bar.png) repeat scroll 0% 0% transparent;
padding-top: 2px;
}

