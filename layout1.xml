color:#FBFBFB;
background-color:#FBFBFB;
padding-bottom:0px;
}
.footer p {
    margin: 20px 0;
  color: #999999;
  float:left;
}
.header-logo {
padding-left: 15px;
padding-top: 15px;
float:left;
}
.footer-logo {
padding-right: 20px;
padding-top: 15px;
float:left;
}

.box-heading {
color: #6E6E6E !important;
font-weight: bold;
}
.navbar-nav {
    margin: 0 -15px !important;
    background-color: #fbfbfb !important;
}
